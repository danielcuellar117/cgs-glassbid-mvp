  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
